    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, del_flag AS delFlag, video_name AS videoName, video_url AS videoUrl, video_introducation AS videoIntroducation, video_author AS videoAuthor, create_time AS createTime, update_time AS updateTime, create_by AS createBy, update_by AS updateBy
    </sql>

</mapper>
