<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ngiveu.cloud.tabe.mapper.BookMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ngiveu.cloud.tabe.entity.Book">
        <id column="id" property="id" />
        <result column="del_flag" property="delFlag" />
        <result column="book_name" property="bookName" />
        <result column="book_author" property="bookAuthor" />
        <result column="book_url" property="bookUrl" />
        <result column="book_avatar" property="bookAvatar" />
        <result column="book_introducation" property="bookIntroducation" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, del_flag AS delFlag, book_name AS bookName, book_author AS bookAuthor, book_url AS bookUrl, book_avatar AS bookAvatar, book_introducation AS bookIntroducation, create_time AS createTime, update_time AS updateTime, create_by AS createBy, update_by AS updateBy
    </sql>

</mapper>
