<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ngiveu.cloud.tabe.mapper.TagMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ngiveu.cloud.tabe.entity.Tag">
        <id column="id" property="id" />
        <result column="tag_name" property="tagName" />
        <result column="tag_description" property="tagDescription" />
        <result column="tag_status" property="tagStatus" />
        <result column="update_time" property="updateTime"/>
        <result column="del_flag" property="delFlag"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tag_name AS tagName, tag_description AS tagDescription, tag_status AS tagStatus, update_time AS updateTime, del_flag AS delFlag
    </sql>

</mapper>
